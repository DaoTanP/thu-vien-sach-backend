@model IEnumerable<QuanLyThuVien.Models.TheThuVien>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MatKhauThe)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayHetHan)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MatKhauThe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayHetHan)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SoThe }) |
            @Html.ActionLink("Details", "Details", new { id=item.SoThe }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SoThe })
        </td>
    </tr>
}

</table>
